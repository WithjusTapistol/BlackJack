# Hearts





twoOfHearts() {

    printf "%s\n" "${redWhite}.-------.${end}"

    printf "%s\n" "${redWhite}|${yellowDarkRed}2${end}${redWhite}.---. |${end}"

    printf "%s\n" "${redWhite}| :${end}${yellowDarkRed}   ${end}${redWhite}: |${end}"

    printf "%s\n" "${redWhite}| :${end}${yellowDarkRed} H ${end}${redWhite}: |${end}"

    printf "%s\n" "${redWhite}| :${end}${yellowDarkRed}   ${end}${redWhite}: |${end}"

    printf "%s\n" "${redWhite}| '---'${yellowDarkRed}2${end}${redWhite}|${end}"

    printf "%s\n" "${redWhite}'-------'${end}"

}

threeOfHearts() {

    printf "%s\n" "${redWhite}.-------.${end}"

    printf "%s\n" "${redWhite}|${yellowDarkRed}3${end}${redWhite}.---. |${end}"

    printf "%s\n" "${redWhite}| :${end}${yellowDarkRed}   ${end}${redWhite}: |${end}"

    printf "%s\n" "${redWhite}| :${end}${yellowDarkRed} H ${end}${redWhite}: |${end}"

    printf "%s\n" "${redWhite}| :${end}${yellowDarkRed}   ${end}${redWhite}: |${end}"

    printf "%s\n" "${redWhite}| '---'${yellowDarkRed}3${end}${redWhite}|${end}"

    printf "%s\n" "${redWhite}'-------'${end}"

}

fourOfHearts() {

    printf "%s\n" "${redWhite}.-------.${end}"

    printf "%s\n" "${redWhite}|${yellowDarkRed}4${end}${redWhite}.---. |${end}"

    printf "%s\n" "${redWhite}| :${end}${yellowDarkRed}   ${end}${redWhite}: |${end}"

    printf "%s\n" "${redWhite}| :${end}${yellowDarkRed} H ${end}${redWhite}: |${end}"

    printf "%s\n" "${redWhite}| :${end}${yellowDarkRed}   ${end}${redWhite}: |${end}"

    printf "%s\n" "${redWhite}| '---'${yellowDarkRed}4${end}${redWhite}|${end}"

    printf "%s\n" "${redWhite}'-------'${end}"

}

fiveOfHearts() {

    printf "%s\n" "${redWhite}.-------.${end}"

    printf "%s\n" "${redWhite}|${yellowDarkRed}5${end}${redWhite}.---. |${end}"

    printf "%s\n" "${redWhite}| :${end}${yellowDarkRed}   ${end}${redWhite}: |${end}"

    printf "%s\n" "${redWhite}| :${end}${yellowDarkRed} H ${end}${redWhite}: |${end}"

    printf "%s\n" "${redWhite}| :${end}${yellowDarkRed}   ${end}${redWhite}: |${end}"

    printf "%s\n" "${redWhite}| '---'${yellowDarkRed}5${end}${redWhite}|${end}"

    printf "%s\n" "${redWhite}'-------'${end}"

}

