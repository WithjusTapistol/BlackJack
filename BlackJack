####################################################################################################################

# Sets colors to be used with printf

# instructions found @ https://unix.stackexchange.com/questions/92563/friendly-terminal-color-names-in-shell-scripts

#

# Example to set color:

# printf "%s\n" "Text in ${red}red${end}, white and ${blu}blue${end}."

#

# Better color guide at https://misc.flogisoft.com/bash/tip_colors_and_formatting

#

####################################################################################################################





# Font color

red=$'\e[91m' # Red

grn=$'\e[92m' # Green

yel=$'\e[93m' # Yellow

blu=$'\e[94m' # Blue

mag=$'\e[95m' # Magenta

cyn=$'\e[96m' # Cyan

pur=$'\e[35m' # Purple

gry=$'\e[37m' # Grey

dred=$'\e[31m' # Dark Red

dgrn=$'\e[32m' # Dark Green

dyel=$'\e[33m' # Dark Yellow

dblu=$'\e[34m' # Dark Blue

dcyn=$'\e[36m' # Dark Cyan

blk=$'\e[30m' # Black



# Background color

backred=$'\e[101m' # Red

backgrn=$'\e[102m' # Green

backyel=$'\e[103m' # Yellow

backblu=$'\e[104m' # Blue

backmag=$'\e[105m' # Magenta

backcyn=$'\e[106m' # Cyan

backpur=$'\e[45m' # Purple

backgry=$'\e[107m' # Grey

backdred=$'\e[41m' # Dark Red

backdgrn=$'\e[42m' # Dark Green

backdyel=$'\e[43m' # Dark Yellow

backdblu=$'\e[44m' # Dark Blue

backdcyn=$'\e[46m' # Dark Cyan

backdgry=$'\e[47m' # Dark Grey



# Font and Background color

redWhite=$'\e[0;31;47m'

whiteDarkGrey=$'\e[0;33;40m'

yellowDarkRed=$'\e[0;33;41m'

blackRed=$'\e[0;30;41m'





# End color

end=$'\e[0m'





welcomeScreen() {

        clear

        sleep 1

        printf "%s" "Welcome to"

        sleep 0.6

        printf "%s" "."

        sleep 0.6

        printf "%s" "."

        sleep 0.6

        printf "%s\n" "."

        sleep 1

        clear

        sleep 1



        echo " "

        printf "%s\n" " /${dred}\$\$\$\$\$\$\$  ${end}/${dred}\$\$                     ${end}/${dred}\$\$                ${end}/${dred}\$\$\$\$\$                     ${end}/${dred}\$\$${end}"

        sleep 0.1

        printf "%s\n" "| ${dred}\$\$${end}__  ${dred}\$\$${end}| ${dred}\$\$                    ${end}| ${dred}\$\$               ${end}|__  ${dred}\$\$                    ${end}| ${dred}\$\$${end}"

        sleep 0.1

        printf "%s\n" "| ${dred}\$\$  ${end}\\ ${dred}\$\$${end}| ${dred}\$\$  ${end}/${dred}\$\$\$\$\$\$   ${end}/${dred}\$\$\$\$\$\$\$${end}| ${dred}\$\$   ${end}/${dred}\$\$            ${end}| ${dred}\$\$  ${end}/${dred}\$\$\$\$\$\$   ${end}/${dred}\$\$\$\$\$\$\$${end}| ${dred}\$\$   ${end}/${dred}\$\$${end}"

        sleep 0.1

        printf "%s\n" "| ${dred}\$\$\$\$\$\$\$ ${end}| ${dred}\$\$ ${end}|____  ${dred}\$\$ ${end}/${dred}\$\$${end}_____/| ${dred}\$\$  ${end}/${dred}\$\$/            ${end}| ${dred}\$\$ ${end}|____  ${dred}\$\$ ${end}/${dred}\$\$${end}_____/| ${dred}\$\$  ${end}/${dred}\$\$${end}/"

        sleep 0.1

        printf "%s\n" "| ${dred}\$\$${end}__  ${dred}\$\$${end}| ${dred}\$\$  ${end}/${dred}\$\$\$\$\$\$\$${end}| ${dred}\$\$      ${end}| ${dred}\$\$\$\$\$\$${end}/        /${dred}\$$  ${end}| ${dred}\$\$  ${end}/${dred}\$\$\$\$\$\$\$${end}| ${dred}\$\$      ${end}| ${dred}\$\$\$\$\$\$${end}/"

        sleep 0.1

        printf "%s\n" "| ${dred}\$\$  ${end}\\ ${dred}\$\$${end}| ${dred}\$\$ ${end}/${dred}\$\$${end}__  ${dred}\$\$${end}| ${dred}\$\$      ${end}| ${dred}\$\$${end}_  ${dred}\$\$       ${end}| ${dred}\$\$  ${end}| ${dred}\$\$ ${end}/${dred}\$\$${end}__  ${dred}\$\$${end}| ${dred}\$\$      ${end}| ${dred}\$\$${end}_  ${dred}\$\$${end}"

        sleep 0.1

        printf "%s\n" "| ${dred}\$\$\$\$\$\$\$${end}/| ${dred}\$\$${end}|  ${dred}\$\$\$\$\$\$\$${end}|  ${dred}\$\$\$\$\$\$\$${end}| ${dred}\$\$ ${end}\\  ${dred}\$\$      ${end}|  ${dred}\$\$\$\$\$\$/${end}|  ${dred}\$\$\$\$\$\$\$${end}|  ${dred}\$\$\$\$\$\$\$${end}| ${dred}\$\$ ${end}\\  ${dred}\$\$${end}"

        sleep 0.1

        printf "%s\n" "|_______/ |__/ \\_______/ \\_______/|__/  \\__/       \\______/  \\_______/ \\_______/|__/  \\__/"

        flashScreenBlackJack

        flashScreenBlackJack

        main

}



flashScreenBlackJack() {





        clear

        # Dark Red

        echo " "

        printf "%s\n" " /${dred}\$\$\$\$\$\$\$  ${end}/${dred}\$\$                     ${end}/${dred}\$\$                ${end}/${dred}\$\$\$\$\$                     ${end}/${dred}\$\$${end}"

        printf "%s\n" "| ${dred}\$\$${end}__  ${dred}\$\$${end}| ${dred}\$\$                    ${end}| ${dred}\$\$               ${end}|__  ${dred}\$\$                    ${end}| ${dred}\$\$${end}"

        printf "%s\n" "| ${dred}\$\$  ${end}\\ ${dred}\$\$${end}| ${dred}\$\$  ${end}/${dred}\$\$\$\$\$\$   ${end}/${dred}\$\$\$\$\$\$\$${end}| ${dred}\$\$   ${end}/${dred}\$\$            ${end}| ${dred}\$\$  ${end}/${dred}\$\$\$\$\$\$   ${end}/${dred}\$\$\$\$\$\$\$${end}| ${dred}\$\$   ${end}/${dred}\$\$${end}"

        printf "%s\n" "| ${dred}\$\$\$\$\$\$\$ ${end}| ${dred}\$\$ ${end}|____  ${dred}\$\$ ${end}/${dred}\$\$${end}_____/| ${dred}\$\$  ${end}/${dred}\$\$/            ${end}| ${dred}\$\$ ${end}|____  ${dred}\$\$ ${end}/${dred}\$\$${end}_____/| ${dred}\$\$  ${end}/${dred}\$\$${end}/"

        printf "%s\n" "| ${dred}\$\$${end}__  ${dred}\$\$${end}| ${dred}\$\$  ${end}/${dred}\$\$\$\$\$\$\$${end}| ${dred}\$\$      ${end}| ${dred}\$\$\$\$\$\$${end}/        /${dred}\$$  ${end}| ${dred}\$\$  ${end}/${dred}\$\$\$\$\$\$\$${end}| ${dred}\$\$      ${end}| ${dred}\$\$\$\$\$\$${end}/"

        printf "%s\n" "| ${dred}\$\$  ${end}\\ ${dred}\$\$${end}| ${dred}\$\$ ${end}/${dred}\$\$${end}__  ${dred}\$\$${end}| ${dred}\$\$      ${end}| ${dred}\$\$${end}_  ${dred}\$\$       ${end}| ${dred}\$\$  ${end}| ${dred}\$\$ ${end}/${dred}\$\$${end}__  ${dred}\$\$${end}| ${dred}\$\$      ${end}| ${dred}\$\$${end}_  ${dred}\$\$${end}"

        printf "%s\n" "| ${dred}\$\$\$\$\$\$\$${end}/| ${dred}\$\$${end}|  ${dred}\$\$\$\$\$\$\$${end}|  ${dred}\$\$\$\$\$\$\$${end}| ${dred}\$\$ ${end}\\  ${dred}\$\$      ${end}|  ${dred}\$\$\$\$\$\$/${end}|  ${dred}\$\$\$\$\$\$\$${end}|  ${dred}\$\$\$\$\$\$\$${end}| ${dred}\$\$ ${end}\\  ${dred}\$\$${end}"

        printf "%s\n" "|_______/ |__/ \\_______/ \\_______/|__/  \\__/       \\______/  \\_______/ \\_______/|__/  \\__/"

        sleep 0.05



        clear

        # Red

        echo " "

        printf "%s\n" " /${red}\$\$\$\$\$\$\$  ${end}/${red}\$\$                     ${end}/${red}\$\$                ${end}/${red}\$\$\$\$\$                     ${end}/${red}\$\$${end}"

        printf "%s\n" "| ${red}\$\$${end}__  ${red}\$\$${end}| ${red}\$\$                    ${end}| ${red}\$\$               ${end}|__  ${red}\$\$                    ${end}| ${red}\$\$${end}"

        printf "%s\n" "| ${red}\$\$  ${end}\\ ${red}\$\$${end}| ${red}\$\$  ${end}/${red}\$\$\$\$\$\$   ${end}/${red}\$\$\$\$\$\$\$${end}| ${red}\$\$   ${end}/${red}\$\$            ${end}| ${red}\$\$  ${end}/${red}\$\$\$\$\$\$   ${end}/${red}\$\$\$\$\$\$\$${end}| ${red}\$\$   ${end}/${red}\$\$${end}"

        printf "%s\n" "| ${red}\$\$\$\$\$\$\$ ${end}| ${red}\$\$ ${end}|____  ${red}\$\$ ${end}/${red}\$\$${end}_____/| ${red}\$\$  ${end}/${red}\$\$/            ${end}| ${red}\$\$ ${end}|____  ${red}\$\$ ${end}/${red}\$\$${end}_____/| ${red}\$\$  ${end}/${red}\$\$${end}/"

        printf "%s\n" "| ${red}\$\$${end}__  ${red}\$\$${end}| ${red}\$\$  ${end}/${red}\$\$\$\$\$\$\$${end}| ${red}\$\$      ${end}| ${red}\$\$\$\$\$\$${end}/        /${red}\$$  ${end}| ${red}\$\$  ${end}/${red}\$\$\$\$\$\$\$${end}| ${red}\$\$      ${end}| ${red}\$\$\$\$\$\$${end}/"

        printf "%s\n" "| ${red}\$\$  ${end}\\ ${red}\$\$${end}| ${red}\$\$ ${end}/${red}\$\$${end}__  ${red}\$\$${end}| ${red}\$\$      ${end}| ${red}\$\$${end}_  ${red}\$\$       ${end}| ${red}\$\$  ${end}| ${red}\$\$ ${end}/${red}\$\$${end}__  ${red}\$\$${end}| ${red}\$\$      ${end}| ${red}\$\$${end}_  ${red}\$\$${end}"

        printf "%s\n" "| ${red}\$\$\$\$\$\$\$${end}/| ${red}\$\$${end}|  ${red}\$\$\$\$\$\$\$${end}|  ${red}\$\$\$\$\$\$\$${end}| ${red}\$\$ ${end}\\  ${red}\$\$      ${end}|  ${red}\$\$\$\$\$\$/${end}|  ${red}\$\$\$\$\$\$\$${end}|  ${red}\$\$\$\$\$\$\$${end}| ${red}\$\$ ${end}\\  ${red}\$\$${end}"

        printf "%s\n" "|_______/ |__/ \\_______/ \\_______/|__/  \\__/       \\______/  \\_______/ \\_______/|__/  \\__/"

        sleep 0.05



        clear

        # Dark Yellow

        echo " "

        printf "%s\n" " /${dyel}\$\$\$\$\$\$\$  ${end}/${dyel}\$\$                     ${end}/${dyel}\$\$                ${end}/${dyel}\$\$\$\$\$                     ${end}/${dyel}\$\$${end}"

        printf "%s\n" "| ${dyel}\$\$${end}__  ${dyel}\$\$${end}| ${dyel}\$\$                    ${end}| ${dyel}\$\$               ${end}|__  ${dyel}\$\$                    ${end}| ${dyel}\$\$${end}"

