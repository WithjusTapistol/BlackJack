        clear

        # Green

        echo " "

        printf "%s\n" " /${grn}\$\$\$\$\$\$\$  ${end}/${grn}\$\$                     ${end}/${grn}\$\$                ${end}/${grn}\$\$\$\$\$                     ${end}/${grn}\$\$${end}"

        printf "%s\n" "| ${grn}\$\$${end}__  ${grn}\$\$${end}| ${grn}\$\$                    ${end}| ${grn}\$\$               ${end}|__  ${grn}\$\$                    ${end}| ${grn}\$\$${end}"

        printf "%s\n" "| ${grn}\$\$  ${end}\\ ${grn}\$\$${end}| ${grn}\$\$  ${end}/${grn}\$\$\$\$\$\$   ${end}/${grn}\$\$\$\$\$\$\$${end}| ${grn}\$\$   ${end}/${grn}\$\$            ${end}| ${grn}\$\$  ${end}/${grn}\$\$\$\$\$\$   ${end}/${grn}\$\$\$\$\$\$\$${end}| ${grn}\$\$   ${end}/${grn}\$\$${end}"

        printf "%s\n" "| ${grn}\$\$\$\$\$\$\$ ${end}| ${grn}\$\$ ${end}|____  ${grn}\$\$ ${end}/${grn}\$\$${end}_____/| ${grn}\$\$  ${end}/${grn}\$\$/            ${end}| ${grn}\$\$ ${end}|____  ${grn}\$\$ ${end}/${grn}\$\$${end}_____/| ${grn}\$\$  ${end}/${grn}\$\$${end}/"

        printf "%s\n" "| ${grn}\$\$${end}__  ${grn}\$\$${end}| ${grn}\$\$  ${end}/${grn}\$\$\$\$\$\$\$${end}| ${grn}\$\$      ${end}| ${grn}\$\$\$\$\$\$${end}/        /${grn}\$$  ${end}| ${grn}\$\$  ${end}/${grn}\$\$\$\$\$\$\$${end}| ${grn}\$\$      ${end}| ${grn}\$\$\$\$\$\$${end}/"

        printf "%s\n" "| ${grn}\$\$  ${end}\\ ${grn}\$\$${end}| ${grn}\$\$ ${end}/${grn}\$\$${end}__  ${grn}\$\$${end}| ${grn}\$\$      ${end}| ${grn}\$\$${end}_  ${grn}\$\$       ${end}| ${grn}\$\$  ${end}| ${grn}\$\$ ${end}/${grn}\$\$${end}__  ${grn}\$\$${end}| ${grn}\$\$      ${end}| ${grn}\$\$${end}_  ${grn}\$\$${end}"

        printf "%s\n" "| ${grn}\$\$\$\$\$\$\$${end}/| ${grn}\$\$${end}|  ${grn}\$\$\$\$\$\$\$${end}|  ${grn}\$\$\$\$\$\$\$${end}| ${grn}\$\$ ${end}\\  ${grn}\$\$      ${end}|  ${grn}\$\$\$\$\$\$/${end}|  ${grn}\$\$\$\$\$\$\$${end}|  ${grn}\$\$\$\$\$\$\$${end}| ${grn}\$\$ ${end}\\  ${grn}\$\$${end}"

