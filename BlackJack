#############################################################################################

# Card methods

#############################################################################################



# Spades



aceOfSpades() {

        printf "%s\n" "${redWhite}.-------.${end}"

        printf "%s\n" "${redWhite}|${whiteDarkGrey}A${end}${redWhite}.---. |${end}"

        printf "%s\n" "${redWhite}| :${end}${whiteDarkGrey}   ${end}${redWhite}: |${end}"

        printf "%s\n" "${redWhite}| :${end}${whiteDarkGrey} S ${end}${redWhite}: |${end}"

        printf "%s\n" "${redWhite}| :${end}${whiteDarkGrey}   ${end}${redWhite}: |${end}"

        printf "%s\n" "${redWhite}| '---'${whiteDarkGrey}A${end}${redWhite}|${end}"

        printf "%s\n" "${redWhite}'-------'${end}"

}



twoOfSpades() {

        printf "%s\n" "${redWhite}.-------.${end}"

        printf "%s\n" "${redWhite}|${whiteDarkGrey}2${end}${redWhite}.---. |${end}"

        printf "%s\n" "${redWhite}| :${end}${whiteDarkGrey}   ${end}${redWhite}: |${end}"

        printf "%s\n" "${redWhite}| :${end}${whiteDarkGrey} S ${end}${redWhite}: |${end}"

        printf "%s\n" "${redWhite}| :${end}${whiteDarkGrey}   ${end}${redWhite}: |${end}"

        printf "%s\n" "${redWhite}| '---'${whiteDarkGrey}2${end}${redWhite}|${end}"

        printf "%s\n" "${redWhite}'-------'${end}"

}



