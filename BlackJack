        printf "%s\n" "| ${dyel}\$\$${end}__  ${dyel}\$\$${end}| ${dyel}\$\$  ${end}/${dyel}\$\$\$\$\$\$\$${end}| ${dyel}\$\$      ${end}| ${dyel}\$\$\$\$\$\$${end}/        /${dyel}\$$  ${end}| ${dyel}\$\$  ${end}/${dyel}\$\$\$\$\$\$\$${end}| ${dyel}\$\$      ${end}| ${dyel}\$\$\$\$\$\$${end}/"

        printf "%s\n" "| ${dyel}\$\$  ${end}\\ ${dyel}\$\$${end}| ${dyel}\$\$ ${end}/${dyel}\$\$${end}__  ${dyel}\$\$${end}| ${dyel}\$\$      ${end}| ${dyel}\$\$${end}_  ${dyel}\$\$       ${end}| ${dyel}\$\$  ${end}| ${dyel}\$\$ ${end}/${dyel}\$\$${end}__  ${dyel}\$\$${end}| ${dyel}\$\$      ${end}| ${dyel}\$\$${end}_  ${dyel}\$\$${end}"

        printf "%s\n" "| ${dyel}\$\$\$\$\$\$\$${end}/| ${dyel}\$\$${end}|  ${dyel}\$\$\$\$\$\$\$${end}|  ${dyel}\$\$\$\$\$\$\$${end}| ${dyel}\$\$ ${end}\\  ${dyel}\$\$      ${end}|  ${dyel}\$\$\$\$\$\$/${end}|  ${dyel}\$\$\$\$\$\$\$${end}|  ${dyel}\$\$\$\$\$\$\$${end}| ${dyel}\$\$ ${end}\\  ${dyel}\$\$${end}"

        printf "%s\n" "|_______/ |__/ \\_______/ \\_______/|__/  \\__/       \\______/  \\_______/ \\_______/|__/  \\__/"

        sleep 0.05



        clear

        # Yellow

        echo " "

        printf "%s\n" " /${yel}\$\$\$\$\$\$\$  ${end}/${yel}\$\$                     ${end}/${yel}\$\$                ${end}/${yel}\$\$\$\$\$                     ${end}/${yel}\$\$${end}"

        printf "%s\n" "| ${yel}\$\$${end}__  ${yel}\$\$${end}| ${yel}\$\$                    ${end}| ${yel}\$\$               ${end}|__  ${yel}\$\$                    ${end}| ${yel}\$\$${end}"

        printf "%s\n" "| ${yel}\$\$  ${end}\\ ${yel}\$\$${end}| ${yel}\$\$  ${end}/${yel}\$\$\$\$\$\$   ${end}/${yel}\$\$\$\$\$\$\$${end}| ${yel}\$\$   ${end}/${yel}\$\$            ${end}| ${yel}\$\$  ${end}/${yel}\$\$\$\$\$\$   ${end}/${yel}\$\$\$\$\$\$\$${end}| ${yel}\$\$   ${end}/${yel}\$\$${end}"

        printf "%s\n" "| ${yel}\$\$\$\$\$\$\$ ${end}| ${yel}\$\$ ${end}|____  ${yel}\$\$ ${end}/${yel}\$\$${end}_____/| ${yel}\$\$  ${end}/${yel}\$\$/            ${end}| ${yel}\$\$ ${end}|____  ${yel}\$\$ ${end}/${yel}\$\$${end}_____/| ${yel}\$\$  ${end}/${yel}\$\$${end}/"

        printf "%s\n" "| ${yel}\$\$${end}__  ${yel}\$\$${end}| ${yel}\$\$  ${end}/${yel}\$\$\$\$\$\$\$${end}| ${yel}\$\$      ${end}| ${yel}\$\$\$\$\$\$${end}/        /${yel}\$$  ${end}| ${yel}\$\$  ${end}/${yel}\$\$\$\$\$\$\$${end}| ${yel}\$\$      ${end}| ${yel}\$\$\$\$\$\$${end}/"

        printf "%s\n" "| ${yel}\$\$  ${end}\\ ${yel}\$\$${end}| ${yel}\$\$ ${end}/${yel}\$\$${end}__  ${yel}\$\$${end}| ${yel}\$\$      ${end}| ${yel}\$\$${end}_  ${yel}\$\$       ${end}| ${yel}\$\$  ${end}| ${yel}\$\$ ${end}/${yel}\$\$${end}__  ${yel}\$\$${end}| ${yel}\$\$      ${end}| ${yel}\$\$${end}_  ${yel}\$\$${end}"

        printf "%s\n" "| ${yel}\$\$\$\$\$\$\$${end}/| ${yel}\$\$${end}|  ${yel}\$\$\$\$\$\$\$${end}|  ${yel}\$\$\$\$\$\$\$${end}| ${yel}\$\$ ${end}\\  ${yel}\$\$      ${end}|  ${yel}\$\$\$\$\$\$/${end}|  ${yel}\$\$\$\$\$\$\$${end}|  ${yel}\$\$\$\$\$\$\$${end}| ${yel}\$\$ ${end}\\  ${yel}\$\$${end}"

